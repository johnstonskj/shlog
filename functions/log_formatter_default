# -*- mode: sh; eval: (sh-set-shell "zsh") -*-

emulate -L zsh

mute_color
printf '%s ' "$(${SHLOG[_DATE_CMD]} --date="@${1}" -u +'%Y-%m-%dT%H:%M:%SZ')"
if [[ -n "${2}" ]]; then
    printf '%s ' "$(_log_scopes_display)"
fi
reset_color

message_level_color "${3}"
printf '[%s] %s %s\n' "${4}" "${5}" "${6}"
reset_color
